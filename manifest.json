{
  "name": "ywr的第一个扩展程序",
  "description": "一个很牛的扩展程序",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "/service-worker/index.js",
    "type":"module"
  },
  "permissions": [
    "tabs",
    "windows",
    "storage",
    "activeTab",
    "scripting",
    "alarms",
    "webNavigation",
    "history"
  ],
  "chrome_url_overrides": {
    "newtab": "/tab/index.html"
  },
  "options_page": "/options/index.html",
  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "64": "/images/icon.png",
      "128": "/images/icon.png"
    }
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "64": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}